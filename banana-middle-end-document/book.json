{
  "plugins": [
    "expandable-chapters",
    "chapter-fold",
    "hide-element",
    "tbfed-pagefooter",
    "-lunr", "-search", "search-pro",
    "splitter",
    "back-to-top-button"
  ],
  "pluginsConfig": {
    "tbfed-pagefooter": {
      "copyright":"banana-2019",
      "modify_label": "修改时间:",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    }
  },
  "links": {
    "gitbook": false,
    "sharing": {
      "google": false,
      "facebook": false,
      "twitter": false,
      "all": false
    }
  }
}